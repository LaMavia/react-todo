turn (\r\n        <main className=\"main\">\r\n          <LoginForm auth={this.state.fb.auth()}/>\r\n        </main>\r\n      )\r\n    } else if(this.state.user){\r\n      return (\r\n        <main className=\"main\">\r\n          Hello {this.state.user.email}\r\n        </main>\r\n      )\r\n    }\r\n  }\r\n}\r\n"},"lineCount":null}},"hash":"b9a64f8c6f4c20d7f748bca477572084","cacheData":{"env":{}}}